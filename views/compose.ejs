<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">

  <div class="form-group">
    <label for="formTitleLabel">Title</label>
    <input type="text" class="form-control" id="formTitleId" name="formTitleName">
  </div>

  <div class="form-group">
    <label for="formPostLabel">Post</label>
    <textarea class="form-control" id="formTextAreaId" rows="7" name="formTextAreaName"></textarea>
  </div>

  <button type="submit" class="btn btn-primary" name="button">Publish</button>

</form>

<%- include("partials/footer") -%>
